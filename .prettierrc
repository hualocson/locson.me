{
  "semi": true,
  "singleQuote": false,
  "tabWidth": 2,
  "printWidth": 80,
  "trailingComma": "es5",
  "arrowParens": "always",
  "plugins": [
    "@trivago/prettier-plugin-sort-imports",
    "prettier-plugin-tailwindcss"
  ],
  "tailwindAttributes": [
    "customClass"
  ],
  "tailwindFunctions": [
    "cva",
    "cn",
    "clsx"
  ],
  "importOrder": [
    "^(react/(.*)$)|^(react$)",
    "^(next/(.*)$)|^(next$)",
    "<THIRD_PARTY_MODULES>",
    "^@/components/layout/(.*)$",
    "^@/components/ui/(.*)$",
    "^@/components/(.*)$",
    "^@/utils/hook(.*)$",
    "^@/utils/(.*)$",
    "^@/store/(.*)$",
    "^[.]",
    "^@heroicons/react/(.*)$",
    ".png",
    ".svg"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}